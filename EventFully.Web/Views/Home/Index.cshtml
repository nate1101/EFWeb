@{
    Layout = null;
    ViewData["Title"] = "EventBx";
}

<!DOCTYPE html>
<html>
<head>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-139919793-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-139919793-1');
    </script>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="EventBx, the simple, affordable event management with both the organizer and attendee in mind." />
    <meta property="og:locale" content="en_US" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="Event Management Services | EventBx" />
    <meta property="og:description" content="EventBx, the simple, affordable event manaegment with both the organizer and attendee in mind." />
    <meta property="og:url" content="https://eventbx.com/" />
    <meta property="og:site_name" content="EventBx" />
    <meta property="og:image" content="https://eventbx.com/images/landing/logo.png" />
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:description" content="EventBx, the simple, affordable event management with both the organizer and attendee in mind." />
    <meta name="twitter:title" content="Event Management Services | EventBx" />
    <meta name="twitter:image" content="https://eventbx.com/images/landing/logo.png" />
    <title>@ViewData["Title"]</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="shortcut icon" href="~/images/landing/favicon.ico">
    <link rel="canonical" href="https://eventbx.com/" />

    <environment names="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.css" />
        <link rel="stylesheet" href="~/css/animate.css" />
        <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/reset.css" asp-append-version="true" />
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.min.css" />
        <link rel="stylesheet" href="~/css/animate.css" />
        <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/reset.css" asp-append-version="true" />
    </environment>

</head>

<body id="page-top" class="landing-page">
    <partial name="_CookieConsentPartial" />
    <div class="navbar-wrapper">
        <nav class="navbar navbar-default navbar-fixed-top navbar-expand-md" role="navigation">
            <div class="container">
                <a class="navbar-brand" href="/">
                    <img src="~/images/landing/logo.png" />
                </a>
                <div class="navbar-header page-scroll">
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar">
                        <i class="fa fa-bars"></i>
                    </button>
                </div>
                <div class="collapse navbar-collapse justify-content-end" id="navbar">
                    <ul class="nav navbar-nav navbar-right">
                        <li><a class="nav-link page-scroll" href="#page-top">Home</a></li>
                        <li><a class="nav-link page-scroll" href="#features">Features</a></li>
                        @*<li><a class="nav-link page-scroll" href="#team">Team</a></li>*@
                        <li><a class="nav-link page-scroll" href="#testimonials">Testimonials</a></li>
                        <li><a class="nav-link page-scroll" href="#pricing">Pricing</a></li>
                        <li><a class="nav-link page-scroll" href="#contact">Contact</a></li>
                    </ul>
                </div>
            </div>
        </nav>
    </div>
    <div id="inSlider" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#inSlider" data-slide-to="0" class="active"></li>
            <li data-target="#inSlider" data-slide-to="1"></li>
        </ol>
        <div class="carousel-inner" role="listbox">
            <div class="carousel-item active">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>
                            Simple, affordable<br />
                            event management<br />
                            with both the organizer<br />
                            and attendee in mind
                        </h1>
                        @*<p>Lorem Ipsum is simply dummy text of the printing.</p>*@
                        <p>
                            <a class="btn btn-lg btn-primary" href="#features" role="button">READ MORE</a>
                        </p>
                    </div>
                    <div class="carousel-image wow zoomIn">
                        <img src="~/Images/landing/laptop.png" alt="laptop" />
                    </div>
                </div>
                <!-- Set background for slide in css -->
                <div class="header-back one"></div>

            </div>
            <div class="carousel-item">
                <div class="container">
                    <div class="carousel-caption blank">
                        <h1>Your event <br /> in the palm of your hand.</h1>
                        <p>Information delivered simply and real-time</p>
                        <p><a class="btn btn-lg btn-primary" href="#features" role="button">Learn more</a></p>
                    </div>s
                </div>
                <!-- Set background for slide in css -->
                <div class="header-back two"></div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#inSlider" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#inSlider" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>


    <section id="features" class="container services">
        <div class="row">
            <div class="col-sm-3">
                <h2>Informative</h2>
                <p>Shouldn't your attendees have access to all facets of your event? Sessions, tracks, registration information, speakers, exhibitors and more all in the palm of their hand.</p>
                @*<p><a class="navy-link" href="#" role="button">Details &raquo;</a></p>*@
            </div>
            <div class="col-sm-3">
                <h2>Simple</h2>
                <p>Setting up an event in our Event Management System should be as easy as 1-2-3. Well, it is. Upload your sessions, tracks and speakers with the push of a button, publish and then fine-tune.</p>
                @*<p><a class="navy-link" href="#" role="button">Details &raquo;</a></p>*@
            </div>
            <div class="col-sm-3">
                <h2>Affordable</h2>
                <p>An event is expensive enough to host and manage. That's why we keep our features simple and our pricing simpler. We offer a single price point that is more than competitive in the market and take the guess-work out of costs.</p>
                @*<p><a class="navy-link" href="#" role="button">Details &raquo;</a></p>*@
            </div>
            <div class="col-sm-3">
                <h2>Intuitive</h2>
                <p>We like to keep our Event Management System and mobile app simple, concise and intuitive. Every task associated with creating an event is clear and simple. With all that goes into an hosting an event, this should be the easy part.</p>
                @*<p><a class="navy-link" href="#" role="button">Details &raquo;</a></p>*@
            </div>
        </div>
    </section>

    <section class="container features">
        <div class="row">
            <div class="col-lg-12 text-center">
                <div class="navy-line"></div>
                <h1>Intuitive and Simple<br /> <span class="navy"> Event-ready mobile experience for your attendees</span> </h1>
                @*<p>Donec sed odio dui. Etiam porta sem malesuada magna mollis euismod. </p>*@
            </div>
        </div>
        <div class="row">
            <div class="col-md-3 text-center wow fadeInLeft">
                <div>
                    <i class="fa fa-calendar features-icon"></i>
                    <h2>Full Schedule</h2>
                    <p>All agenda items are available in an easy to navigate calendar with track filtering.</p>
                </div>
                <div class="m-t-lg">
                    <i class="fa fa-clock-o features-icon"></i>
                    <h2>Reminders</h2>
                    <p>Never miss out on a session with list and push notification reminders.</p>
                </div>
                <div class="m-t-lg">
                    <i class="fa fa-handshake-o features-icon"></i>
                    <h2>Sponsors</h2>
                    <p>Highlight your sponsors with business and contact information.</p>
                </div>
            </div>
            <div class="col-md-6 text-center  wow zoomIn">
                <img src="~/Images/landing/dashboard.png" alt="dashboard" class="img-fluid dashboard-image">
            </div>
            <div class="col-md-3 text-center wow fadeInRight">
                <div>
                    <i class="fa fa-calendar-check-o features-icon"></i>
                    <h2>Personalized Agenda</h2>
                    <p>Customize your own schedule by adding agenda items to your own personal calendar.</p>
                </div>
                <div class="m-t-lg">
                    <i class="fa fa-users features-icon"></i>
                    <h2>Speakers</h2>
                    <p>Get to know the event's speakers, their background and the sessions they will be leading.</p>
                </div>
                <div class="m-t-lg">
                    <i class="fa fa-building features-icon"></i>
                    <h2>Exhibitors</h2>
                    <p>Let your attendees know about Exhibitors and the products and services they have to offer.</p>
                </div>
            </div>
        </div>
    </section>
    <section class="container features">
        <div class="row">
            <div class="col-lg-12 text-center">
                <div class="navy-line"></div>
                <h1>Event Management Features</h1>
                <p>Here are just some of the features of our intuitive Event Management System</p>
            </div>
        </div>
        <div class="row features-block">
            <div class="col-lg-4 features-text wow fadeInLeft">
                @*<small>INSPINIA</small>*@
                <h2><strong>Event Dashboard</strong></h2>
                <p>The centerpoint of your event, this is the launch point for all your event's features and information.</p>
                @*<a href="" class="btn btn-primary">Learn more</a>*@
            </div>
            <div class="col-lg-8 text-right wow fadeInRight">
                <img src="~/Images/landing/dashboard-s.png" alt="dashboard" class="img-fluid float-right">
            </div>
        </div>
        <div class="row features-block">
            <div class="col-lg-8 text-right wow fadeInRight">
                <img src="~/Images/landing/schedule-s.png" alt="dashboard" class="img-fluid float-right">
            </div>
            <div class="col-lg-4 features-text wow fadeInLeft">
                @*<small>INSPINIA</small>*@
                <h2><strong>Schedule</strong></h2>
                <p>Easily add items to your schedule along with speakers and tracks. All in one place! Displayed in an easy-to-use agenda format that allows you to visualize your event schedule from beginning to end.</p>
                @*<a href="" class="btn btn-primary">Learn more</a>*@
            </div>
        </div>
        <div class="row features-block">
            <div class="col-lg-4 features-text wow fadeInLeft">
                @*<small>INSPINIA</small>*@
                <h2><strong>Schedule Upload</strong></h2>
                <p>Download our schedule template and enter your event's sessions, tracks, speakers and locations directly into a spreadsheet. Upload it using our simple interface and do the housekeeping. Voila!</p>
                @*<a href="" class="btn btn-primary">Learn more</a>*@
            </div>
            <div class="col-lg-8 text-right wow fadeInRight">
                <img src="~/Images/landing/upload-s.png" alt="dashboard" class="img-fluid float-right">
            </div>
        </div>
        <div class="row features-block">
            <div class="col-lg-8 text-right wow fadeInRight">
                <img src="~/Images/landing/speaker-s.png" alt="dashboard" class="img-fluid float-right">
            </div>
            <div class="col-lg-4 features-text wow fadeInLeft">
                @*<small>INSPINIA</small>*@
                <h2><strong>Speakers</strong></h2>
                <p>Highlight your speakers by entering background, contact information and bios. You can even assign the speakers to agenda items all in one place!</p>
                @*<a href="" class="btn btn-primary">Learn more</a>*@
            </div>
        </div>
    </section>

